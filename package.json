    "@typescript-eslint/parser": "^6.7.3",
    "argparse": "^2.0.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "3.0.3",
    "rollup": "3.20.0",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "jimp": "^0.22.10"
